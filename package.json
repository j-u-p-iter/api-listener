{
  "name": "@j.u.p.iter/api-listener",
  "version": "1.1.0",
  "description": "Listen api until it gives you, what you want",
  "keys": ["api", "listener", "api listener", "wait", "wait for response"],
  "main": "dist/lib/api-listener.js",
  "module": "dist/lib/api-listener.esm.js",
  "umd:main": "dist/lib/api-listener.umd.js",
  "source": "dist/lib/api-listener.js",
  "typings": "dist/types",
  "repository": "git@github.com:j-u-p-iter/api-listener.git",
  "author": "Leontyev Kirill (j.u.p.iter) <pn79122821224@gmail.com> (http://j-u-p-iter.pro/)",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build:modules": "rollup -c rollup.config.ts",
    "build": "yarn run build:modules",
    "start": "yarn run build:modules -w",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "tslint -p tsconfig.json -t codeFrame 'src/**/*.ts'",
    "test": "jest --config jest.config.js",
    "test:watch": "yarn run test --watch",
    "test:prod": "yarn run lint && yarn run test --coverage --no-cache"
