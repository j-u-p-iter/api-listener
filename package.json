{
  "name": "@j.u.p.iter/api-listener",
  "version": "0.0.0-semantically-released",
  "description": "Listen api until it gives you, what you want",
  "keywords": [
    "api",
    "listener",
    "api listener",
    "wait",
    "wait for response"
  ],
  "main": "dist/lib/api-listener.cjs.js",
  "module": "dist/lib/api-listener.esm.js",
  "umd:main": "dist/lib/api-listener.umd.min.js",
  "source": "dist/lib/api-listener.js",
  "typings": "dist/types",
  "repository": "git@github.com:j-u-p-iter/api-listener.git",
  "author": "Leontyev Kirill (j.u.p.iter) <pn79122821224@gmail.com> (http://j-u-p-iter.pro/)",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "commit": "jupiter-scripts precommit --allowJs",
    "build:modules": "jupiter-scripts build --bundle",
    "build": "yarn run build:modules",
    "start": "yarn run build:modules --watch",
    "lint": "jupiter-scripts lint --format codeFrame",
    "lint:fix": "yarn run lint --fix",
    "test": "jupiter-scripts test",
    "test:prod": "yarn run lint && yarn run test --coverage --no-cache",
    "test-coverage-report": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release --prepare && npm publish && semantic-releaese --success",
    "travis-deploy-once": "travis-deploy-once"
  },
  "husky": {
    "hooks": {
      "pre-commit": "jupiter-scripts pre-commit"
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "dependencies": {
    "commitizen": "^3.0.4",
    "typescript": "^3.2.1"
  },
  "devDependencies": {
    "@j.u.p.iter/jupiter-scripts": "^1.0.5",
    "coveralls": "^3.0.2",
    "semantic-release": "^15.12.0",
    "travis-deploy-once": "^5.0.9"
  }
}
